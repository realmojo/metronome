{
  "expo": {
    "name": "메트로놈 코드",
    "slug": "metronome",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/metronome-logo.png",
    "scheme": "metronome",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mojoday.metronome"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/metronome-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mojoday.metronome"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/metronome-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/metronome-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-1963334904140891~6693481993",
          "iosAppId": "ca-app-pub-1963334904140891~6693481993"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "adUnitId": {
        "banner": "ca-app-pub-1963334904140891~6693481993"
      }
    }
  }
}
